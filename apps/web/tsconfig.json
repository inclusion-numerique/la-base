{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "esModuleInterop": true,
    "strictNullChecks": true,
    "module": "esnext",
    "types": ["cypress", "jest", "node"],
    "paths": {
      "@lb/cli/*": ["../../apps/cli/src/*"],
      "@lb/web/*": ["src/*"],
      "@lb/cdk/*": ["../../packages/cdk/src/*"],
      "@lb/config/*": ["../../packages/config/src/*"],
      "@lb/emails/*": ["../../packages/emails/src/*"],
      "@lb/lint/*": ["../../packages/lint/src/*"],
      "@lb/scaleway/*": ["../../packages/cdk/.gen/providers/scaleway/*"],
      "@lb/storybook/*": ["../../packages/storybook/src/*"],
      "@lb/test/*": ["../../packages/test/src/*"],
      "@lb/ui/*": ["../../packages/ui/src/*"]
    },
    "baseUrl": ".",
  },
  "include": [
    "next-env.d.ts",
    "node_modules/@types/testing-library__jest-dom/index.d.ts",
    "cypress/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts",
    "nextAuth.d.ts"
  ]
}
