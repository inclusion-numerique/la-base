{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "cypress",
      "cypress-real-events",
      "@testing-library/cypress",
      "node"
    ],
    "paths": {
      "@app/cli/*": [
        "../../apps/cli/src/*"
      ],
      "@app/web/*": [
        "../../apps/web/src/*"
      ],
      "@app/cdk/*": [
        "../../packages/cdk/src/*"
      ],
      "@app/config/*": [
        "../../packages/config/src/*"
      ],
      "@app/e2e/*": [
        "./cypress/*"
      ],
      "@app/emails/*": [
        "../../packages/emails/src/*"
      ],
      "@app/lint/*": [
        "../../packages/lint/src/*"
      ],
      "@app/scaleway/*": [
        "../../packages/cdk/.gen/providers/scaleway/*"
      ],
      "@app/storybook/*": [
        "../../packages/storybook/src/*"
      ],
      "@app/test/*": [
        "../../packages/test/src/*"
      ],
      "@app/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@app/fixtures/*": [
        "../../packages/fixtures/src/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "./cypress.config.ts",
    "./cypress/**/*.ts",
    "./cypress/**/*.tsx"
  ]
}
